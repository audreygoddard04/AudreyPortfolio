{
  "buildCommand": "cd my-app && npm install && npm run build",
  "outputDirectory": "my-app/build",
  "installCommand": "cd my-app && npm install",
  "framework": null,
  "functions": {
    "my-app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/my-app/api/$1"
    }
  ]
}

